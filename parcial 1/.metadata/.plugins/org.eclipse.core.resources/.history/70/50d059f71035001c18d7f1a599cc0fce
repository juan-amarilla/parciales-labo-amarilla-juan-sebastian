#include "ClienteInformes.h"

void mostrarCliente(eCliente unCliente)
{
	 printf("%5d  %12s %22s %20s %20s %20d \n\n", unCliente.id,
			                                      unCliente.nombre,
												  unCliente.cuit,
												  unCliente.direccion,
												  unCliente.localidad,
												  unCliente.contadorPendiente);

}
